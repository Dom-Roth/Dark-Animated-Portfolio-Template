{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2021",
    "moduleResolution": "node",
    "pretty": true,
    "noImplicitAny": true,
    "removeComments": false,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": false,
    "allowSyntheticDefaultImports": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "sourceRoot": "ts",
    "baseUrl": "./",
    "outDir": "../static/js/index.js",
    "paths": {
      "@portfoliowebsite/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules", "test/**/*.d.ts", "src/**/*.d.ts"]
}
